<?xml version="1.0" encoding="UTF-8"?>
<test-set xmlns="http://www.w3.org/2010/09/qt-fots-catalog"
          name="misc-UCACollation" covers-40="PR1093">
  <description>Tests for fn:collation()</description>
   
  <dependency type="spec" value="XP40+ XQ40+"/>


  <test-case name="fn-collation-001">
      <description>strength Primary</description>
      <created by="Michael Kay" on="2024-03-17"/>
      <test>compare('abc','aBC',collation(map{'strength':'primary','fallback':false()}))</test>
      <result>
         <any-of>
            <assert-eq>0</assert-eq>
            <error code="FOCH0002"/>
         </any-of>
      </result>
   </test-case>
   
